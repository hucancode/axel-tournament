{
  "name": "frontend",
  "version": "0.0.1",
  "devDependencies": {
    "typescript": "^5.9.3",
    "svelte-check": "^4.3.4"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "bun run build/index.js",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
